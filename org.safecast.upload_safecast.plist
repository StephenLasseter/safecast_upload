<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" \
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<dict>
        <key>Label</key>
        <string>org.safecast.upload_safecast</string>
	<key>EnvironmentVariables</key>
	<dict>
		<key>PATH</key>
		<string>/usr/local/share/python:/usr/local/bin:/bin:/usr/bin:/usr/sbin</string>
	</dict>
	<key>EnableGlobbing</key>
	<true/>
        <key>ProgramArguments</key>
        <array>
		<string>bash</string>
		<string>-c</string>
		<string>~/Safecast/bin/upload_safecast >> ~/Library/Logs/upload_safecast.log 2>> ~/Library/Logs/upload_safecast_err.log</string>
        </array>
	<key>StartOnMount</key>
	<true/>
</dict>
</plist>
